<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
	parameterType: 들어가는값 (객체)
	resultType: 나오는 값 (객체)
-->

<mapper namespace="test">
	<!-- java.lang.: 생략가능 -->
	<select id="checkmember" parameterType = "java.lang.String" resultType = "Integer">
		SELECT COUNT(*)
		FROM EMPLOYEES
		WHERE EMPLOYEE_ID = #{id}
	</select>
</mapper>